<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.com.wl.mapper.UserMapper">
	<select id="findUserById" parameterType="int" resultType="cn.com.wl.entry.User">
		SELECT * FROM USER WHERE id=#{value}
</select>
	<select id="findAll" resultType="cn.com.wl.entry.User">
		SELECT * FROM  USER;
	</select>
</mapper>


